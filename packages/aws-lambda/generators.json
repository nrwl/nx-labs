{
  "$schema": "http://json-schema.org/schema",
  "name": "aws-lamda",
  "version": "0.0.1",
  "generators": {
    "serverless": {
      "factory": "./src/generators/serverless/serverless",
      "schema": "./src/generators/serverless/schema.json",
      "description": "Creates a new project with AWS Lambda configuration"
    },
    "setup-serverless": {
      "factory": "./src/generators/setup-serverless/setup-serverless",
      "schema": "./src/generators/setup-serverless/schema.json",
      "description": "Adds AWS Lamda Serverless configuration to an existing project"
    },
    "preset": {
      "factory": "./src/generators/preset/preset",
      "schema": "./src/generators/preset/schema.json",
      "description": "Nx AWS Lambda Serverless preset generator",
      "hidden": true
    }
  }
}
